{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/predict",
      "destination": "/api/index.py"
    },
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ]
}
